r\n  outline: none;\r\n}\r\ninput::placeholder {\r\n  color: var(--desaturatedRed);\r\n  font-size: 12px;\r\n}\r\n\r\nbutton {\r\n  width: 25%;\r\n  position: absolute;\r\n  right: 0;\r\n  height: 42px;\r\n  border-radius: 20px;\r\n  border: none;\r\n  background-image: linear-gradient(\r\n    100deg,\r\n    rgba(243, 241, 241, 0.7) 0%,\r\n    rgba(248, 69, 69, 0.7) 100%\r\n  );\r\n  outline: none;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: var(--softRed);\r\n}\r\n\r\n.attribution {\r\n  width: 100%;\r\n}\r\n@media screen and (max-width: 750px) {\r\n  main.hero-section {\r\n    flex-direction: column;\r\n    background-size: cover;\r\n  }\r\n\r\n  .hero-text {\r\n    order: 2;\r\n  }\r\n  .hero-image {\r\n    display: none;\r\n  }\r\n  .hero-image-mobile {\r\n    display: block;\r\n    margin: 7rem 0 0;\r\n    height: 100%;\r\n    width: 100%;\r\n  }\r\n\r\n  .logo {\r\n    position: absolute;\r\n    width: 110px;\r\n    height: 35px;\r\n    top: 3rem;\r\n    left: 2rem;\r\n    margin-bottom: 0;\r\n  }\r\n\r\n  .hero-text {\r\n    width: 80%;\r\n    margin: 4rem auto;\r\n    text-align: center;\r\n    justify-content: center;\r\n    justify-items: center;\r\n  }\r\n\r\n  h1 {\r\n    text-align: center;\r\n    font-size: 3rem;\r\n    width: 100%;\r\n  }\r\n\r\n  p {\r\n    width: 100%;\r\n    text-align: center;\r\n  }\r\n  .error-message {\r\n    margin: 10px 5px;\r\n    color: var(--softRed);\r\n    font-size: 12px;\r\n    visibility: hidden;\r\n    text-align: left;\r\n  }\r\n  form {\r\n    width: 100%;\r\n    justify-content: center;\r\n    margin-top: 3rem;\r\n  }\r\n\r\n  input {\r\n    width: 100%;\r\n  }\r\n\r\n  button {\r\n    width: 30%;\r\n  }\r\n  .icon-error {\r\n    position: absolute;\r\n    right: 31%;\r\n    top: 0.5rem;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 400px) {\r\n  h1 {\r\n    font-size: 2.1rem;\r\n    width: 100%;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"262549fb9682849f86c7c60cc82274